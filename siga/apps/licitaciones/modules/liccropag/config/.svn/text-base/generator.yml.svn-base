      numcont:        { name: Número Contrato, params: size=10 maxlength=8 readonly=true style=border:none }
      resolu:         { name: Resolución, params: size=10 maxlength=8 readonly=true style=border:none }
      fecrel:         { name: Fecha Resolución,         params: readonly=true style=border:none }
      numptocuecon:
        name: Número Punto de Cuenta Contratación
        msj_error: El Código es necesario
        params: readonly=true style=border:none
