      ejepre:         { name: Ejercicio Prespuestario Año, params: style="border:none" size=80}
      desclacomp:     { name: Solicitud de Compra, params: style="border:none" size=80}
      destiplic:      { name: Modalidad de Contratación, params: style="border:none" size=80}
      tipcom:         { name: Compra, params: style="border:none" size=80}
      tipcon:         { name: Bienes/servicios, params: style="border:none" size=80}
      codpro:         { name: Codigo Empresa, params: style="border:none" size=80}
      nompro:         { name: Empresa, params: style="border:none" size=80}
      rifpro:         { name: R.I.F, params: readonly=true style=border:none}
      dirpro:         { name: Dirección , params: readonly=true style=border:none}
      nomrepleg:      { name: Representante Legal, params: readonly=true style=border:none size=80}
      numofe:         { name: Número Oferta, params: size=10 readonly=true style=border:none }
      fecofe:         { name: Fecha Oferta, params: readonly=true style=border:none}
      monofe:         { name: Monto Oferta, params: readonly=true style=border:none }
      monofelet:      { name: Son, params: readonly=true style=border:none size=80 }
      fecreg:         { name: Fecha Registro, params: readonly=true style=border:none}
      dias:           { name: Dias para el Vencimiento, params: readonly=true style=border:none}
      fecven:         { name: Fecha Vencimiento, params: readonly=true style=border:none}
      objcont:        { name: Objeto del Contrato, type: textarea_tag, params: size="80x10" readonly=true style=border:none}
      monart:         { name: Monto Partida de Obra, params: readonly=true style=border:none }
      monrec:         { name: Monto Recargos, params: readonly=true style=border:none }
      gridart:        { name: .}
      gridrgo:        { name: .}
      gridforpag:     { name: .}
      gridcroent:     { name: .}
      gridfia:        { name: .}
      gridactas:      { name: .}
      sancio:         { name: Sanciones, type: textarea_tag, params: size="80x10" readonly=true style=border:none}
      garant:         { name: Garantias referentes al Bien o Servicio, type: textarea_tag, params: size="80x10" readonly=true style=border:none}
      docane1:
        name:  Documento Anexo 1
#        type:  admin_input_file_tag
        params:  include_link=assets include_text="*********Anexo 1***********" include_remove=true readonly=true style=border:none
        upload_dir:  assets
        help:       Favor cargar solo archivos con extension (pdf,doc,ppt)
      docane2:
        name:  Documento Anexo 2
#        type:  admin_input_file_tag
        params:  include_link=assets include_text="*********Anexo 2***********" include_remove=true readonly=true style=border:none
        upload_dir:  assets
        help:       Favor cargar solo archivos con extension (pdf,doc,ppt)
      docane3:
        name:  Documento Anexo 3
#        type:  admin_input_file_tag
        params:  include_link=assets include_text="*********Anexo 3***********" include_remove=true readonly=true style=border:none
        upload_dir:  assets
        help:       Favor cargar solo archivos con extension (pdf,doc,ppt)
      prepor:         { name: Preparado Por, params: size=80 readonly=true style=border:none }
      preporcar:      { name: Cargo, params: size=80 readonly=true style=border:none }
      lisicact_id:    { name: Declaratoria Modalidad, params: include_blank="Seleccione..." disabled=true}
      fecdecla:       { name: Fecha Declaratoria, params: readonly=true style=border:none}
      detdecmod:      { name: Detalle, type: textarea_tag, params: size="80x10" readonly=true style=border:none}
      anapor:         { name: Analisado Por, params: size=80 readonly=true style=border:none}
      anaporcar:      { name: Cargo, params: size=80 readonly=true style=border:none}
      js:             {name: .}
      totcont:        {name: Total Contrato}
      totpag:        {name: Total Pagado}
      totporpag:        {name: Total Por Pagar}

    list:
      title:           Cronograma de Pagos
      display:         [=numcont, numexp, desuniadm, desuniste, fecreg, rifpro, nompro, numptocuecon ]
      filters:         [numcont, numexp, coduniadm, coduniste, fecreg]
      fields:
        desuniadm:     { name: Para}
        desuniste:     { name: De}
        numexp:        { name: Número de Expediente, params: readonly=false style=border:display }
        numcont:       { name: Número Contrato, params: size=10 maxlength=8 readonly=false style=border:display }
        coduniadm:
          name: Unidad
          msj_error: El Código es necesario
          params: readonly=false style=border:display
        coduniste:
          name: Unidad
          msj_error: El Código es necesario
          params: readonly=false style=border:display
        fecreg:         { name: Fecha Registro, params: readonly=false style=border:display}


      object_actions:
        _edit:          ~
        _delete:        ~
      max_per_page:    10
      sort:            numcont
      actions:
        NONE


    edit:
      title:           Cronograma de Pagos
      display:
        Resumen Contrato:    [ente, codempadm, nomcaradm, coduniadm, codempeje, nomcareje, coduniste, numcont, resolu, fecrel, numptocuecon]
        Datos Generales:    [numrecofe, numexp, ejepre, destiplic, desclacomp, tipcom, codpro, nompro, rifpro, nomrepleg, dirpro, numofe, fecofe, monofe, monofelet, fecreg, dias, fecven, objcont]
        Resumen Pagos:  [monofe, totpag, totporpag]
        Formas de Pago: [_gridforpag]
        Actas:   [_gridactas]        
#        Cronograma de Entregas:  [_gridcroent]
        Otros Datos:        [sancio, garant, docane1, docane2, docane2, prepor, preporcar]
        Declaratoria:       [lisicact_id, fecdecla, detdecmod, anapor, anaporcar, _js]
      actions:
        _list:        ~
        _save:        ~

