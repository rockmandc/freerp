generator:
  class:              cidesaPropelAdminGenerator
  param:
    model_class:      Bndefins
    theme:            cidesaGenerator
    fields:
      codins: { name: Código}
      nomins: { name: Nombre}
      dirins: { name: Dirección}
      telins: { name: Telefono}
      faxins: { name: Fax}
      email: { name: Email}
      edoins: { name: Estado}
      munins: { name: Municipio}
      paqins: { name: Parroquia}
      foract: { name: Formato}
      desact: { name: Descripción}
      lonact: { name: Longitud de Activo}
      forubi: { name: Formato}
      desubi: { name: Descripción}
      lonubi: { name: Longitud de Ubicación}
      fecper: { name: Período}
      feceje: { name: Ejercicio}
      coddes: { name: Desincorporación}
      codinc: { name: Incorporación}
      porrev: { name: Valor}
      corrmue: { name: Disposición de Muebles}
      corrinm: { name: Inmuebles}
      corrsem: { name: Semovientes}
      coractmue:  { name: Bienes Muebles}
      coractinm:  { name: Bienes Inmuebles}
      porpri:  { name: Valor}
      codtiptra:  { name: Tipo de Transacción}
      cormuenacd:  { name: Desde}
      cormuenach:  { name: Hasta}
      cormueregd:  { name: Desde}
      cormueregh:  { name: Hasta}      
      modcormue:  { name: Modificar el Correlativo del Mueble}      
    list:
      title:          Definición de Empresa
      display:        [=codins, nomins]
      fields:
        fecper: { params: date_format='dd/MM/yy' }
        feceje: { params: date_format='dd/MM/yy' }
      object_actions:
        _edit:        ~
    edit:
      title:          Edición de Empresa
      display:       [codins, nomins, dirins, telins, faxins, email, edoins, munins, paqins, foract, desact, forubi, desubi, fecper, feceje, coddes, porrev, corrmue, corrinm, corrsem, codinc, coractmue, coractinm, porpri, codtiptra, cormuenacd, cormueregd, cormuenach, cormueregh, modcormue]
      actions:
        _save:        ~
      fields:
        fecper: { params: date_format='dd/MM/yy' }
        feceje: { params: date_format='dd/MM/yy' }
