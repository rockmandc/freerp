generator:
  class:              sfPropelAdminGenerator
  param:
    model_class:      Bndefconi
    theme:            default
    fields:
      codact:      { name : Desde }
      codact1:      { name : Hasta }      
      codinm:      { name : Código Activo }
      desinm:      { name : Descripción }

      ctadepcar:   { name : Cuenta a Cargar }
      descta:      { name : Descripción Cuenta a Cargar }
      ctadepabo:   { name : Cuenta a Abonar }
