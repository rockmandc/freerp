methods:
  post:
   - "fordeforgpub{codorg}"
   - "fordeforgpub{nomorg}"   
names: 
  fordeforgpub{codorg}:
    required:  Yes
    required_msg: El c√≥digo del organismo no puedo estar en Blanco
    validators: Validator_codorg
    
  fordeforgpub{nomorg}:
    required:  Yes
    required_msg: El nombre del organismo no puedo estar en Blanco
    validators: Validator_nomorg       

Validator_codorg:
    class: sfPropelUniqueValidator
    param:
        class:        Fordeforgpub
        column:       codorg
        unique_error: El registro ya Existe intente con otro.
    
Validator_nomorg:
    class: sfStringValidator
    param:
        min:       2
        min_error: El nombre no puede tener menos de 2 caracteres
        max:       100
        max_error: El nombre no puede pasar mas de 100 caracteres