<script type="text/javascript">
 function enter(e,valor)
 {
   if (e.keyCode==13 || e.keyCode==9)
   {
     if (valor!='')
     { valor=valor.pad(10, '0',0);}
     else{valor=valor.pad(10, '#',0);}

     $('viarensolvia_numren').value=valor;
   }
 }

if ($('id').value!='') 
	$('viarensolvia_numren').readOnly=true;
</script>