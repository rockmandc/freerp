			$obj2 = new $cls();
			$obj2->hydrate($rs, $startcol);

			$newObject = true;
			foreach($results as $temp_obj1) {
				$temp_obj2 = $temp_obj1->getLiaddendum(); 				if ($temp_obj2->getPrimaryKey() === $obj2->getPrimaryKey()) {
					$newObject = false;
										$temp_obj2->addLiregaddcondet($obj1); 					break;
				}
			}
			if ($newObject) {
				$obj2->initLiregaddcondets();
				$obj2->addLiregaddcondet($obj1); 			}
