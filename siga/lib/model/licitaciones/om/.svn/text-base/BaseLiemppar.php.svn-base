	{
		switch($pos) {
			case 0:
				$this->setNumplie($value);
				break;
			case 1:
				$this->setNumexp($value);
				break;
			case 2:
				$this->setCodpro($value);
				break;
			case 3:
				$this->setFecret($value);
				break;
			case 4:
				$this->setNomrepleg($value);
				break;
			case 5:
				$this->setRifpro($value);
				break;
