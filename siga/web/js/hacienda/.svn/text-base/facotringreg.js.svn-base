function activarCont()
{
	if (confirm('El Contribuyente no Existe desea incluirlo'))
         {
             if (confirm('Solo salvara algunos datos del Contribuyente, el resto debe ser llenado en el módulo ')){
              $('fcotring_nomcon').readOnly= false;
              $('fcotring_nomcon').focus();
              $('fcotring_dircon').readOnly=false;
              $('fcotring_nacconcon_V').readOnly=false;
              $('fcotring_nacconcon_E').readOnly=false;
              $('fcotring_nacconcon_V').checked=true;
              $('fcotring_tipconcon_N').readOnly=false;
              $('fcotring_tipconcon_J').readOnly=false;
              $('fcotring_tipconcon_N').checked=true;
          }
          }
}
function activarRep()
{
	if (confirm('El Representante no Existe desea incluirlo'))
         {
            if (confirm('Solo salvara algunos datos del Representante, el resto debe ser llenado en el módulo '))
         {
              $('fcotring_nomconrep').readOnly= false;
              $('fcotring_nomconrep').focus();
              $('fcotring_dirconrep').disabled= false;
              $('fcotring_nacconrep_V').readOnly=false;
              $('fcotring_nacconrep_E').readOnly=false;
              $('fcotring_nacconrep_V').checked=true;
              $('fcotring_tipconrep_N').readOnly=false;
              $('fcotring_tipconrep_J').readOnly=false;
              $('fcotring_tipconrep_N').checked=true;
          }
          }
}




