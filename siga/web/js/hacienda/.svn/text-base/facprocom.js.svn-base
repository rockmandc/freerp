function activarCont()
{
	if (confirm('El Contribuyente no Existe desea incluirlo'))
         {
             if (confirm('Solo salvara algunos datos del Contribuyente, el resto debe ser llenado en el módulo ')){
              $('fcprolic_nomcon').readOnly= false;
              $('fcprolic_nomcon').focus();
              $('fcprolic_dircon').readOnly=false;
              $('fcprolic_nacconcon_V').readOnly=false;
              $('fcprolic_nacconcon_E').readOnly=false;
              $('fcprolic_nacconcon_V').checked=true;
              $('fcprolic_tipconcon_N').readOnly=false;
              $('fcprolic_tipconcon_J').readOnly=false;
              $('fcprolic_tipconcon_N').checked=true;
          }
          }
}
function activarRep()
{
	if (confirm('El Representante no Existe desea incluirlo'))
         {
            if (confirm('Solo salvara algunos datos del Representante, el resto debe ser llenado en el módulo '))
         {
              $('fcprolic_nomconrep').readOnly= false;
              $('fcprolic_nomconrep').focus();
              $('fcprolic_dirconrep').disabled= false;
              $('fcprolic_nacconrep_V').readOnly=false;
              $('fcprolic_nacconrep_E').readOnly=false;
              $('fcprolic_nacconrep_V').checked=true;
              $('fcprolic_tipconrep_N').readOnly=false;
              $('fcprolic_tipconrep_J').readOnly=false;
              $('fcprolic_tipconrep_N').checked=true;
          }
          }
}


