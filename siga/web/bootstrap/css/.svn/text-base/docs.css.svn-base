div.pie {
    margin-top: -26px;
    margin-left: -30px;
}


div.page-head {
    background-image: url("/images/fph.png");
    background-repeat: no-repeat;
    background-position: top right;
    height: 80px;
    margin: 0 0;
    color: white;
    width: 425px;
    float: left;
    display: block;
}

div.page-head h1{
    font-weight: normal;
    margin: 0 0;
    font-size: 30px;
    padding: 15px 15px;
}

div.span3 {
    width: 270px;
}
div.span7 {
    margin-left: 5px;
    width: 725px;
    
}
div.pie {
    width: 1000px;
    height: 250px;
    background: transparent url("/images/bloq-pie.png") no-repeat left top;
    position: relative;
}


div.pie .head-pie { 
    padding-top: 1px;
    width: 100%;
    margin-top: 30px;
    text-align: center;
    font-size: 18px;
    color: #0000FF;
}

div.pie .head-pie p{
    margin-top: 12px;
}

div.pie .intereses {
    margin-top: -10px;
    position: relative;
    width: 100%;
    height: 150px;
     padding: 10px 15px;
}

div.pie .intereses .item-interes {
    float: left;
    width: 22%;   
    margin-left: 15px;
    margin-top: 15px;
}

div.pie .intereses .item-interes img{
    width: 30%;
    float: left;
}
div.pie .intereses .item-interes .infor {
    width: 69%;
    height: 60px;
    float: left;
    margin-top: 0px;
    margin-left: 0px;
    text-align: center;
    background: transparent url("/images/bii.png") no-repeat center center;

}
div.pie .intereses .item-interes .infor:hover{
    background: transparent url("/images/biih.png") no-repeat center center;
}

div.pie .intereses .item-interes .infor a {
    text-decoration: none;
    padding: 5px 15px;
}
div.pie .intereses .item-interes .infor p{
    margin: 20px 10px;
    color: #0000ff;
    font-size: 12px;
}

